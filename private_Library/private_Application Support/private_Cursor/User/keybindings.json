[
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "alt+cmd+b",
    "command": "-r.runFromBeginningToLine",
    "when": "editorTextFocus && editorLangId == 'r'"
  },
  {
    "key": "alt+cmd+e",
    "command": "-r.runFromLineToEnd",
    "when": "editorTextFocus && editorLangId == 'r'"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  },
  {
    "key": "h",
    "command": "editor.action.scrollLeftHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "j",
    "command": "editor.action.scrollDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "k",
    "command": "editor.action.scrollUpHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "l",
    "command": "editor.action.scrollRightHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl-u",
    "command": "editor.action.pageUpHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl-d",
    "command": "editor.action.pageDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": ", d a",
    "command": "r.loadAll",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r"
  },
  {
    "key": ", d c",
    "command": "r.check",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r"
  },
  {
    "key": ", d d",
    "command": "r.document",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r"
  },
  {
    "key": ", d t",
    "command": "r.test",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r"
  },
  {
    "key": ", l",
    "command": "r.runSelectionRetainCursor",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
  },
  {
    "key": ", t b",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "traceback()"
  },
  {
    "key": ", t m",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_make()"
  },
  {
    "key": ", t shift+m",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_make(callr_function = NULL)"
  },
  {
    "key": ", t t",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_unblock_process(store = '_targets'); targets::tar_make(names = '$$', callr_function = NULL, use_crew = FALSE, as_job = FALSE)"
  },
  {
    "key": ", t shift+t",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_unblock_process(store = '_targets'); targets::tar_make(names = '$$')"
  },
  {
    "key": ", t r",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_load($$)"
  },
  {
    "key": ", t p",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_read($$)"
  },
  {
    "key": ", t l",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "targets::tar_source()"
  },
  {
    "key": ", s q",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "Q"
  },
  {
    "key": ", s c",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "c"
  },
  {
    "key": ", s s",
    "command": "r.runSelectionRetainCursor",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
  },
  {
    "key": ", r a",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal'|| vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "args($$)"
  },
  {
    "key": ", r p",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "print($$)"
  },
  {
    "key": ", r shift+p",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "print(.Last.value, n = Inf)"
  },
  {
    "key": ", r shift+p",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "print($$, n = Inf)"
  },
  {
    "key": ", r t",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "str($$)"
  },
  {
    "key": ", r n",
    "command": "r.names",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
  },
  {
    "key": ", r l",
    "command": "r.length",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
  },
  {
    "key": ", r h",
    "command": "r.helpPanel.openForSelection",
    "when": "(vim.mode == 'Normal') && (editorLangId =~ /r|rmd|qmd/ && editorTextFocus)",
  },
  {
    "key": ", r f",
    "command": "r.attachActive",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
  },
  {
    "key": ", r r",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "options(width = cli::console_width())"
  },
  {
    "key": ", r q",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "q()"
  },
  {
    "key": ", r m",
    "command": "r.runCommandWithSelectionOrWord",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "base::rm($$)"
  },
  {
    "key": ", r shift+m",
    "command": "r.runCommand",
    "when": "(vim.mode == 'Normal' || vim.mode == 'Visual') && (editorTextFocus || !inputFocus) && editorLangId == r",
    "args": "base::rm(list = ls())"
  },
  {
    "key": "alt+-",
    "command": "type",
    "when": "(vim.mode == 'Insert') && (editorLangId =~ /r|rmd|qmd/ && editorTextFocus)",
    "args": {
      "text": " <- "
    }
  },
  {
    "key": "alt+=",
    "command": "type",
    "when": "(vim.mode == 'Insert') && (editorLangId =~ /r|rmd|qmd/ && editorTextFocus)",
    "args": {
      "text": " %>%"
    }
  },
  {
    "key": "alt+\\",
    "command": "type",
    "when": "(vim.mode == 'Insert') && (editorLangId =~ /r|rmd|qmd/ && editorTextFocus)",
    "args": {
      "text": " |>"
    }
  },
]
