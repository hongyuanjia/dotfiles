{
    "always_save_history": false,
    "check_for_updates": false,
    "code_completion_characters": 2,
    "code_completion_delay": 10,
    "console_double_click_select": true,
    "default_encoding": "UTF-8",
    "default_project_location": "~/Desktop",
    "default_sweave_engine": "knitr",
    "editor_keybindings": "vim",
    "editor_theme": "Tomorrow Night 80s",
    "full_project_path_in_window_title": true,
    "highlight_r_function_calls": true,
    "highlight_selected_line": true,
    "initial_working_directory": "~",
    "insert_numbered_latex_sections": true,
    "jobs_tab_visibility": "shown",
    "line_ending_conversion": "posix",
    "load_workspace": false,
    "num_spaces_for_tab": 4,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "rainbow_parentheses": true,
    "rmd_chunk_output_inline": false,
    "save_workspace": "never",
    "shiny_viewer_type": "browser",
    "show_doc_outline_rmd": true,
    "show_hidden_files": true,
    "show_indent_guides": true,
    "show_terminal_tab": false,
    "source_with_echo": true,
    "strip_trailing_whitespace": true,
    "sync_files_pane_working_dir": true,
    "syntax_color_console": true,
    "tab_multiline_completion": true,
    "use_tinytex": true,
    "vertically_align_arguments_indent": false,
    "visual_markdown_editing_wrap": "column",
    "visual_markdown_editing_wrap_at_column": 80,
    "windows_terminal_shell": "win-ps"
}
